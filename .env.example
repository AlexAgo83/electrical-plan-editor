# Local runtime host for Vite dev/preview servers.
APP_HOST=127.0.0.1

# Dev server port (default expected by local docs/workflow).
APP_PORT=5284

# Preview server port for production build checks.
PREVIEW_PORT=5285

# Playwright base URL (should remain coherent with APP_HOST + APP_PORT).
E2E_BASE_URL=http://127.0.0.1:5284

# Client-side localStorage namespace (safe public value, never a secret).
VITE_STORAGE_KEY=electrical-plan-editor.state
